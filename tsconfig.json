{
  "compilerOptions": {
    "target": "esnext", 
    "module": "esnext", 
    "outDir": "./build", 
    "moduleResolution": "node",
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true, 
    "strict": true, 
    "noUnusedLocals": true, 
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "typeRoots": ["./src/models/types/"]
  }
}
